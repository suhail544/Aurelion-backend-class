datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model blinkit {
  product_id            String @id @default(cuid())
  product_name          String
  product_weight        String
  product_offer         String
  product_price         String
  product_delivery_time String
}
